<odoo>

<record id="student_template_action" model="ir.actions.act_window">
<field name="name">Student</field>
<field name="type">ir.actions.act_window</field>
<field name="res_model">student_details</field>
<field name="view_mode">kanban,tree,form</field>
<field name="help" type="html">
<p class="o_view_nocontent_smiling_face"> Create a new student </p>
<p> create a student. </p>
</field>
</record>
<record id="student_template_search_view" model="ir.ui.view">
<field name="name">student.search</field>
<field name="model">student_details</field>
<field name="arch" type="xml">

<search string="student">

	<field name='id' />
	<field name='name'/>

	

	<field name="roll_no" />

	<field name="Dob" />

	<field name="age" />

	<field name="year"/>

</search>
</field>
</record>
<record id="student_template_tree_view" model="ir.ui.view">
<field name="name">student.tree</field>
<field name="model">student_details</field>
<field name="arch" type="xml">
<tree>

	<field name='id' string='uniqueID'/>
	<field name='name' string='Name'/>

	

	<field name="roll_no" string="Roll_no"/>

	<field name="Dob" string="Dob"/>

	<field name="age" string="Age"/>

	<field name="year" string="year"/>

	<field name="batch" string='batch'/>

</tree>
</field>
</record>
<record id="student_template_form_view" model="ir.ui.view">
<field name="name">student.form</field>
<field name="model">student_details</field>
<field name="arch" type="xml">
<form>
<sheet>
<group>
	<field name='id' string='uniqueID'/>
	<field name='name' string='Name'/>
</group>
<group>
	<field name="image" widgets="image" class="oe_avatar"/>
</group>
<group>
	<field name="roll_no" string="Roll_no"/>
</group>
<group>
	<field name="Dob" string="Dob"/>
</group>
<group>
	<field name="age" string="Age"/>
</group>
<group>
	<field name="year" string="year"/>
</group>
<group>
	<field name="batch" string='batch'/>
</group>
</sheet>
</form>
</field>
</record>
